{
  "name": "serverless-api",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "civ6-save-parser": "1.1.12",
    "dynamoose": "0.8.6",
    "express": "4.16.2",
    "generate-password": "1.4.0",
    "inversify": "4.11.1",
    "inversify-binding-decorators": "3.2.0",
    "jsonwebtoken": "8.2.0",
    "lodash": "4.17.5",
    "moment": "2.21.0",
    "passport": "0.4.0",
    "passport-steam": "1.0.9",
    "pydt-shared": "^1.0.0",
    "reflect-metadata": "0.1.12",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "rollbar": "^2.3.1",
    "source-map-support": "0.5.3",
    "tsoa": "2.1.4",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/hapi": "^17.0.4",
    "@types/lodash": "^4.14.80",
    "@types/node": "^9.4.6",
    "@types/webpack-env": "^1.13.2",
    "co": "^4.6.0",
    "export-api-url": "file:local_modules/export-api-url",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-loader": "^0.5.5",
    "rimraf": "^2.6.2",
    "serverless-domain-manager": "^1.1.21",
    "serverless-mocha-plugin": "^1.7.0",
    "serverless-offline": "^3.16.0",
    "serverless-webpack": "4.4.0",
    "ts-loader": "3.5.0",
    "tslint": "^5.8.0",
    "tslint-no-circular-imports": "^0.2.0",
    "typescript": "^2.5.3",
    "webpack": "3.11.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf api/_gen .webpack",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "bash -c 'mkdir -p api/_gen/routes' && tsoa routes",
    "lint": "tslint --type-check --project tsconfig.json ./api/**/*.ts ./lib/**/*.ts",
    "build": "npm run clean && npm run routes-gen && npm run lint && npm run swagger-gen",
    "start": "npm run build && bash -c \"RUNNING_LOCAL=true sls offline -c -o 0.0.0.0\"",
    "debug": "npm run build && bash -c \"RUNNING_LOCAL=true ./debug-sls.sh offline -c -o 0.0.0.0\"",
    "clean-lambda": "bash -c \"AWS_REGION=us-east-1 node -e \\\"require('./_scripts/cleanLambda').clean();\\\"",
    "deploy-dev": "npm run build && bash -c \"sls deploy -v\"",
    "deploy-prod": "npm run build && bash -c \"WEB_URL=https://www.playyourdamnturn.com sls deploy -v --stage prod\"",
    "backup-prod": "python _scripts/dynamodump/dynamodump.py -m backup -s prod* -r us-east-1",
    "copy-backup-to-dev": "python _scripts/dynamodump/dynamodump.py -m restore -s prod* -d dev* -r us-east-1 --dataOnly && bash _scripts/reset-dev-emails.sh",
    "copy-prod-to-dev": "npm run backup-prod && npm run copy-backup-to-dev",
    "encrypt-config": "openssl des3 -salt -in config.yml -out config.yml.encrypted",
    "decrypt-config": "openssl des3 -d -salt -in config.yml.encrypted -out config.yml"
  }
}
